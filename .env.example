# ---NextJS Frontend---
# The API URL for your specific Supabase project (e.g. for local, http://localhost:54321)
NEXT_PUBLIC_SUPABASE_URL=
# The publishable key for client-side requests, subject to Row Level Security (RLS)
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY=

# ---NextJS Server-side Environment---
# The 'service_role' key for admin access, bypasses RLS (NEVER expose to client)
SUPABASE_SERVICE_ROLE_KEY=

# ---supabase/config.toml---
# The base URL for authentication redirects (e.g. for local, http://localhost:3000)
SUPABASE_AUTH_SITE_URL=
# Google OAuth Client ID from Google Cloud Console
SUPABASE_AUTH_GOOGLE_CLIENT_ID=
# Google OAuth Client Secret from Google Cloud Console
SUPABASE_AUTH_EXTERNAL_GOOGLE_SECRET=